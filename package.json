{
  "name": "caleva-data-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "build:final": "npm run clean && npm run build && npm run copy:package && npm run copy:certs && npm run install:prod && npm run zip",
    "clean": "rimraf dist",
    "copy:src": "copyfiles -u 1 \"src/**/*\" \"*.js\" dist",
    "copy:package": "copyfiles package.json package-lock.json dist",
    "copy:certs": "copyfiles -u 0 \"certs/**/*\" dist",
    "install:prod": "cd dist && npm ci --omit=dev && cd ..",
    "zip": "bestzip file-server.zip dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "bestzip": "^2.2.1",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^11.0.0",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
